@model TravelTripProje.Models.Siniflar.Restorantlar
@{
    ViewBag.Title = "YeniRestorant";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Yeni Restorant Ekleme</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Baslik)
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Konum)
    @Html.TextBoxFor(x => x.Konum, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.RestorantImage)
    @Html.TextBoxFor(x => x.RestorantImage, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Restorant Ekle</button>
</form>


